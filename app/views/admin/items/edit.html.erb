<div>
  <div>
    <div>
      <h5>商品編集</h5>
        <%= form_with model: @item, url: new_admin_item_path do |f| %>

           <label>商品画像</label><%= f.file_field :image, accept: "image/*" %>

          <label>商品名</label> <%= f.text_field :name %>

          <label>商品説明</label> <%= f.text_area :introduction %>

          <label>ジャンル</label>

          <label>税抜価格</label> <%= f.text_field :price %>円

          <label>販売ステータス</label>

          <%= f.submit '変更を保存' %>
        <% end %>
    </div>
  </div>
</div>
